{
    "version": 2,
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "frontend/public"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "frontend/public/index.html"
      },
      {
        "src": "/api/(.*)", 
        "dest": "backend/server.js" 
      }
    ]
  }
  